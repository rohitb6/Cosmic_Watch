# Environment variables for docker-compose
# Copy this to .env and fill in your actual values

# Database
DB_PASSWORD=password

# API Keys
NASA_API_KEY=DEMO_KEY

# Security
SECRET_KEY=your-super-secret-key-change-in-production-12345678

# Debug Mode
DEBUG=True

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Frontend
FRONTEND_PORT=3000

# Redis
REDIS_HOST=redis
REDIS_PORT=6379
